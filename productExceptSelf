class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
int n=nums.size();
vector<int>result(n);
result[0]=1;
for(int i=1;i<n;i++)
{
    result[i]=nums[i-1]*result[i-1];
}
int suffixproduct=1;
for(int i=n-1;i>=0;i--)
{
    result[i]*=suffixproduct;
    suffixproduct*=nums[i];
}

return result;






    }
};
